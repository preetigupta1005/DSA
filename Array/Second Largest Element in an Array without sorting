class Solution {
    public int secondLargestElement(int[] nums) {
        int max = Integer.MIN_VALUE;
        int secondMax = Integer.MIN_VALUE;

        for (int i = 0; i < nums.length; i++) {
            if (nums[i] > max) {
                secondMax = max;
                max = nums[i];
            } else if (nums[i] > secondMax && nums[i] != max) {
                secondMax = nums[i];
            }
        }

        return secondMax == Integer.MIN_VALUE ? -1 : secondMax; // if all ele same[5,5,5,5,5] so secondMa is -1 insead of min value
    }
}
